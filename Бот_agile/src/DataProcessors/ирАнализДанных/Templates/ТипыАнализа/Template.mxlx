						<v8:item>
							<v8:lang>ru</v8:lang>
							<v8:content>АнализДанныхКластеризация</v8:content>
						</v8:item>
						<v8:item>
							<v8:lang>en</v8:lang>
							<v8:content>АнализДанныхКластеризация</v8:content>
						</v8:item>
						<v8:item>
